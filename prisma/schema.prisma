model competitor_comparison_history {
  competitor_mapping_id       BigInt
  our_price                   Float?
  our_currency                String?  @db.VarChar(10)
  competitor_price            Float?
  competitor_currency         String?  @db.VarChar(10)
  price_difference            Float?
  price_difference_percentage Float?
  comparison_date             DateTime @default(now()) @db.Timestamptz(6)
  data_source                 String?  @db.VarChar(100)
  competitor_category_rank    BigInt?
  competitor_sales_rank       BigInt?
  competitor_stock_status     String?  @db.VarChar(50)
  id                          BigInt   @id @default(autoincrement())
  our_category_rank           BigInt?
  our_sales_rank              BigInt?
  our_stock_status            String?  @db.VarChar(50)

  @@index([comparison_date], map: "idx_comparison_history_date")
  @@index([competitor_mapping_id], map: "idx_comparison_history_mapping_id")
}

model competitor_product_mappings {
  id                        BigInt    @id @default(autoincrement())
  our_seller_sku            String    @db.VarChar(100)
  our_asin                  String?   @db.VarChar(20)
  our_product_name          String?   @db.VarChar(500)
  our_marketplace_id        String?   @db.VarChar(20)
  competitor_asin           String    @db.VarChar(20)
  competitor_seller_id      String?   @db.VarChar(100)
  competitor_seller_name    String?   @db.VarChar(255)
  competitor_product_name   String?   @db.VarChar(500)
  competitor_marketplace_id String?   @db.VarChar(20)
  mapping_reason            String?   @db.VarChar(255)
  mapping_notes             String?
  mapping_priority          Int?      @default(1)
  is_active                 Boolean   @default(true)
  created_by_user_id        BigInt?
  verified_by_user_id       BigInt?
  last_price_check          DateTime? @db.Timestamptz(6)
  last_ranking_check        DateTime? @db.Timestamptz(6)
  created_at                DateTime  @default(now()) @db.Timestamptz(6)
  updated_at                DateTime  @default(now()) @db.Timestamptz(6)
  verified_at               DateTime? @db.Timestamptz(6)

  @@unique([our_seller_sku, competitor_asin])
  @@index([is_active], map: "idx_competitor_mappings_active")
  @@index([competitor_asin], map: "idx_competitor_mappings_competitor_asin")
  @@index([created_at], map: "idx_competitor_mappings_created_at")
  @@index([our_asin], map: "idx_competitor_mappings_our_asin")
  @@index([our_seller_sku], map: "idx_competitor_mappings_our_sku")
  @@index([mapping_priority], map: "idx_competitor_mappings_priority")
}

enum role_enum {
  USER
  ADMIN
}
